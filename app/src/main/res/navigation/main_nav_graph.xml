<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main_nav_graph"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/user_list_fragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/user_list_fragment"
        android:name="fr.vannsuplabs.formationkotlin.ui.fragment.UserListFragment"
        android:label="fragment_user_list"
        tools:layout="@layout/fragment_user_list" >
        <action
            android:id="@+id/action_user_list_fragment_to_userDetailFragment"
            app:destination="@id/userDetailFragment"/>
    </fragment>

    <fragment
        android:id="@+id/userDetailFragment"
        android:name="fr.vannsuplabs.formationkotlin.ui.fragment.UserDetailFragment"
        android:label="UserDetailFragment"
        tools:layout="@layout/fragment_user_detail">
    </fragment>

</navigation>